{
  "action": {
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png",
      "64": "img/icon_64.png"
    },
    "default_popup": "popup.html"
  },
  "author": "Jean Lucas Lima <github.com/jeanlucaslima>",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      }
    }
  },
  "description": "A tab organizer",
  "icons": {
    "128": "img/icon_128.png",
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "48": "img/icon_48.png"
  },
  "manifest_version": 3,
  "name": "tabber",
  "optional_host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "version": "1.0"
}
